{
  "shortcut": "Control+Space",
  "shortcutID": "example-menu",
  "centered": false,
  "root": {
    "type": "submenu",
    "name": "示例菜单",
    "icon": "award_star",
    "iconTheme": "material-symbols-rounded",
    "children": [
      {
        "type": "submenu",
        "name": "应用",
        "icon": "apps",
        "iconTheme": "material-symbols-rounded",
        "children": [
          {
            "type": "command",
            "data": {
              "command": "open -a Safari"
            },
            "name": "Safari",
            "icon": "safari",
            "iconTheme": "simple-icons"
          },
          {
            "type": "command",
            "data": {
              "command": "open -a Mail"
            },
            "name": "邮件",
            "icon": "mail",
            "iconTheme": "material-symbols-rounded"
          },
          {
            "type": "command",
            "data": {
              "command": "open -a Music"
            },
            "name": "音乐",
            "icon": "itunes",
            "iconTheme": "simple-icons"
          },
          {
            "type": "command",
            "data": {
              "command": "open -a Finder"
            },
            "name": "访达",
            "icon": "folder_shared",
            "iconTheme": "material-symbols-rounded"
          },
          {
            "type": "command",
            "data": {
